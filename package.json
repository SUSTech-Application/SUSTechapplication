{
  "name": "sustechapplication",
  "type": "module",
  "scripts": {
    "build": "./scripts/bracket-routes-fix.sh && vitepress build",
    "clean": "rm -rf .vitepress/dist/ .vitepress/cache && find . -name \"*.paths.js\" -delete",
    "dev": "vitepress dev",
    "format": "prettier -c .",
    "format:fix": "prettier -w .",
    "lint": "bun run lint:es && bun run lint:md",
    "lint:es": "eslint",
    "lint:es:fix": "eslint --fix",
    "lint:md": "markdownlint-cli2 \"**.md\"",
    "lint:md:fix": "bun run lint -- --fix",
    "preview": "vitepress preview",
    "sort": "bunx sort-package-json",
    "spellcheck": "cspell .",
    "test": "vitest"
  },
  "dependencies": {
    "@primeuix/themes": "^1.0.0",
    "primevue": "^4.3.2"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/ajv": "^1.0.4",
    "@types/node": "^22.13.13",
    "ajv": "^8.17.1",
    "cspell": "^8.17.5",
    "eslint": "^9.21.0",
    "eslint-plugin-vue": "^10.0.0",
    "jiti": "^2.4.2",
    "markdown-it-task-lists": "^2.1.1",
    "markdownlint-cli2": "^0.17.2",
    "prettier": "^3.5.2",
    "prettier-plugin-sh": "^0.15.0",
    "typescript-eslint": "^8.25.0",
    "vitepress": "^1.6.3",
    "vitest": "^3.0.9",
    "yaml": "^2.7.0"
  }
}
